<!----------Create volume factor popup---------->
<div class="pp" id="cvfpp">

    <div class="ppContent" id="cvfppContent">

        <div class="ppIcons">
            <div class="ppIcon2"><i class="fa-regular fa-square-plus"></i></div>
            <div class="ppClose2" id="cvfppClose"><i class="fa-solid fa-rectangle-xmark ppCloseIcon"></i></div>
        </div>

        <div class="ppTitleS mbl" id="cvfppTitle"></div>       
        
        <div class="divInputsColumn">
            <div class="divInput">
                <label class="label2" id="vfppSupplierLabel">Proveedor</label>
                <select class="input1 il ipm"  id="cvfppSupplier"></select>
                <div class="divError" id="cvfppSupplierError"></div>
            </div>
            <div class="divInput mts">
                <label class="label2" id="cvfppStdVolumeLabel">Volúmen estándar</label>
                <input type="number" step="0.01" class="input1 il ipm" id="cvfppStdVolume" autocomplete="off">
                <div class="divError" id="cvfppStdVolumeError"></div>
            </div>
            <div class="divInput mts">
                <label class="label2" id="cvfppStdVolumeUMLabel">UM volúmen estándar</label>
                <select class="input1 il ipm"  id="cvfppStdVolumeMU">
                    <option value=""></option>
                    <option value="m3">m3</option>
                    <option value="f3">f3</option>
                </select>
                <div class="divError" id="cvfppStdVolumeUMError"></div>
            </div>
            <div class="divInput mts">
                <label class="label2" id="cvfppStdCtnLabel">Contenedor estándar</label>
                <input type="number" step="0.01" class="input1 il ipm" id="cvfppStdCtn" autocomplete="off">
                <div class="divError" id="cvfppStdCtnError"></div>
            </div>
            <div class="divInput mts">
                <label class="label2" id="cvfppStdFreightLabel">Flete</label>
                <div class="divFactorData ixxl ipl">
                    <input type="number" step="0.01" class="input1 ixs ips" id="cvfppStdFreight">
                    <div class="factorDataText" id="cvfppStdVolumeMUtext1"> -- </div>
                    <input type="text" class="input1 unabledInput ixs ips" id="cvfppFreight" readonly>
                    <div class="factorDataText" id="cvfppMU1"></div>
                    <div class="divError" id="cvfppStdFreightError"></div>
                </div>
            </div>
            <div class="divInput mtm">
                <label class="label2" id="cvfppStdTerminalExpensesLabel">Gastos terminal</label>
                <div class="divFactorData ixxl ipl">
                    <input type="number" step="0.01" class="input1 ixs ips" id="cvfppStdTerminalExpenses">
                    <div class="factorDataText" id="cvfppStdVolumeMUtext2"> -- </div>
                    <input type="text" class="input1 unabledInput ixs ips" id="cvfppTerminalExpenses" readonly>
                    <div class="factorDataText" id="cvfppMU2"></div>
                    <div class="divError" id="cvfppStdTerminalExpensesError"></div>
                </div>
            </div>
            <div class="divInput mtm">
                <label class="label2" id="cvfppStdDispatchExpensesLabel">Gastos despacho</label>
                <div class="divFactorData ixxl ipl">
                    <input type="number" step="0.01" class="input1 ixs ips" id="cvfppStdDispatchExpenses">
                    <div class="factorDataText" id="cvfppStdVolumeMUtext3"> -- </div>
                    <input type="text" class="input1 unabledInput ixs ips" id="cvfppDispatchExpenses" readonly>
                    <div class="factorDataText" id="cvfppMU3"></div>
                    <div class="divError" id="cvfppStdDispatchExpensesError"></div>
                </div>
            </div>
            <div class="divInput mtm">
                <label class="label2" id="cvfppStdMaritimeAgencyExpensesLabel">Gastos agencia marítima</label>
                <div class="divFactorData ixxl ipl">
                    <input type="number" step="0.01" class="input1 ixs ips" id="cvfppStdMaritimeAgencyExpenses">
                    <div class="factorDataText" id="cvfppStdVolumeMUtext3"> -- </div>
                    <input type="text" class="input1 unabledInput ixs ips" id="cvfppMaritimeAgencyExpenses" readonly>
                    <div class="factorDataText" id="cvfppMU3"></div>
                    <div class="divError" id="cvfppStdMaritimeAgencyExpensesError"></div>
                </div>
            </div>
            <div class="divInput mtm">
                <label class="label2" id="cvfppStdDomesticFreightLabel">Flete interno</label>
                <div class="divFactorData ixxl ipl">
                    <input type="number" step="0.01" class="input1 ixs ips" id="cvfppStdDomesticFreight">
                    <div class="factorDataText" id="cvfppStdVolumeMUtext3"> -- </div>
                    <input type="text" class="input1 unabledInput ixs ips" id="cvfppDomesticFreight" readonly>
                    <div class="factorDataText" id="cvfppMU3"></div>
                    <div class="divError" id="cvfppStdDomesticFreightError"></div>
                </div>
            </div>
            <div class="divInput mtm">
                <label class="label2" id="cvfppTotalVolumeExpensesLabel">Gastos totales por volumen</label>
                <input type="text" class="input1 il ipm" id="cvfppTotalVolumeExpenses" autocomplete="off" readonly>
                <div class="divError" id="cvfppTotalVolumeExpensesError"></div>
                <div class="divInput mtm">
                    <label class="label2" id="cvfppTotalVolumeExpensesLabel">Agente aduana %</label>
                    <input type="text" class="input1 il ipm" id="cvfppTotalVolumeExpenses" autocomplete="off" readonly>
                    <div class="divError" id="cvfppTotalVolumeExpensesError"></div>
                </div>
            </div>
        </div>

        

        <div class="divButtonsRow mtl mbs">
            <div id="cvfppCreate" class="button1 sButton">Crear</div>
            <div id="cvfppEdit" class="button1 sButton">Editar</div>
        </div>
    </div>
</div>

<div class="okPopup" id="cvfppOk">    
    <div class="okContent" id="cvfppOkContent">
        <div class="okDiv">
            <div class="okIcon1"><i class="fa-regular fa-circle-check"></i></div>
            <div class="okText" id="cvfppOkText">Tasa de cambio editada con éxito</div>
        </div>
    </div>
</div>
