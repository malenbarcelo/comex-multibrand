<!----------Create coeficient factor popup---------->
<div class="pp" id="ccfpp">

    <div class="ppContent" id="ccfppContent">

        <div class="ppIcons">
            <div class="ppIcon2"><i class="fa-regular fa-square-plus"></i></div>
            <div class="ppClose2" id="ccfppClose"><i class="fa-solid fa-rectangle-xmark ppCloseIcon"></i></div>
        </div>

        <div class="ppTitleS mbl" id="ccfppTitle"></div>       
        
        <div class="divInputsColumn">
            <div class="divInput">
                <label class="label3" id="ccfppSupplierLabel">Proveedor</label>
                <select class="input1 il ipl"  id="ccfppSupplier"></select>
                <div class="divError2" id="ccfppSupplierError">Debe seleccionar un proveedor</div>
            </div>
            <div class="divInput mtm">
                <label class="label3" id="ccfppFactorLabel">Factor de multipliación</label>
                <input type="number" step="0.01" class="input1 il ipl" id="ccfppFactor" autocomplete="off">
                <div class="divError2" id="ccfppFactorError">Debe completar el volúmen estándar</div>
            </div>
            <div class="divInput mtm">
                <label class="label3" id="ccfppSalesMarginLabel">Margen de venta %</label>
                <input type="number" step="0.01" class="input1 il ipl" id="ccfppSalesMargin" autocomplete="off">
                <div class="divError2" id="ccfppSalesMarginError">Debe completar el margen de venta</div>
            </div>
        </div>

        <div class="divButtonsRow mtl mbs">
            <div id="ccfppCreate" class="button1 mButton"></div>
        </div>
    </div>
</div>

<div class="okPopup" id="ccfppOk">    
    <div class="okContent" id="ccfppOkContent">
        <div class="okDiv">
            <div class="okIcon1"><i class="fa-regular fa-circle-check"></i></div>
            <div class="okText" id="ccfppOkText">Tasa de cambio editada con éxito</div>
        </div>
    </div>
</div>
